{
  "name": "building-safety-project",
  "version": "1.0.0",
  "description": "This project shows building experts and starters good on site practices to adhere to while on site",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx server.ts",
    "dev:watch": "tsx watch server.ts",
    "start": "NODE_ENV=production node server.ts",
    "debug": "ndb server.ts",
    "build": "tsc",
    "clean": "powershell -Command \"Get-ChildItem -Path . -Include *.js,*.d.ts,*.js.map,*.d.ts.map -Recurse | Where-Object { $_.FullName -notmatch 'node_modules|public' -and $_.Name -ne 'xss-clean.d.ts' } | Remove-Item -Force\""
  },
  "author": "Joe Praise",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "sharp": "^0.33.1",
    "slugify": "^1.6.6",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/validator": "^13.15.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
